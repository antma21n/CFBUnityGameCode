week = 1
westRR = RoundRobin(west)
eastRR = RoundRobin(east)
CrossDivisionRR = RoundRobin(west and east) #for annual week 10 CD games

for team in S ECteams:
	while week < 13:
		if week == 1 or 2 or 3:
			Opponent = SelectOOC()

		if week == 4:
			Opponent = westRR[1] #if west
			Opponent = eastRR[1] #if east

		if week == 5:
			Opponent = westRR[2]
			Opponent = eastRR[2]

		if week == 6:
			Opponent = westRR[3]
			Opponent = eastRR[3]

		if week == 7:
			Opponent = westRR[4]
			Opponent = eastRR[4]

		if week == 8:
			CDRival = CrossDivisionRival(team)
			if team == 'arkansas'
				Opponent = 'bye'
			if team == 'mizzou'
				Opponent = 'bye'

		if week == 9:
			Opponent = westRR[5]
			Opponent = eastRR[5]

		if week == 10: #larger round robin for random teams
			Opponent = CrossDivisionRR()
			#Opponent = SelectRCD(CDRival) #except Cross division rival

		if week == 11:
			Opponent = westRR[6]
			Opponent = eastRR[6]

		if week = 12:
			Opponent = SelectOOC()
			if team == 'kentucky'
				Opponent = 'florida'
			if team == 'georgia'
				Opponent = 'south carolina'
			if team == 'florida'
				Opponent = 'kentucky'
			if team == 'south carolina'
				Opponent = 'georgia'

		if week == 13:
			Opponent = RivalWeek[0] 

		week = week + 1